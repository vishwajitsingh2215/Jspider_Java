package com;

public class Solution {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("EVEN numbers ");
		for(int i=2;i<=10;i=i+2)
		{	
			
			System.out.println(i);
		}
		System.out.println("------------------");
		System.out.println("ODD numbers ");
		for(int i=1;i<=9;i+=2) {
			
			System.out.println(i);
		}
	}

}
